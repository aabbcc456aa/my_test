<h1>Editing file_upload</h1>

<% form_for(@file_upload) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :url %><br />
    <%= f.text_field :url %>
  </p>
  <p>
    <%= f.label :file_type %><br />
    <%= f.text_field :file_type %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @file_upload %> |
<%= link_to 'Back', file_uploads_path %>