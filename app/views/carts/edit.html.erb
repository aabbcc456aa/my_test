<h1>Editing cart</h1>

<% form_for(@cart) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :item_id %><br />
    <%= f.text_field :item_id %>
  </p>
  <p>
    <%= f.label :item_num %><br />
    <%= f.text_field :item_num %>
  </p>
  <p>
    <%= f.label :item_total_price %><br />
    <%= f.text_field :item_total_price %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @cart %> |
<%= link_to 'Back', carts_path %>